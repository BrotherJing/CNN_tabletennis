  type: "Reshape"
  bottom: "sodlt_fc8"
  top: "sodlt"
  reshape_param{
    shape{
      dim: 0
      dim: 1
      dim: 50
      dim: -1
    }
  }
}
layer {
  name: "sodlt_norm"
  type: "Sigmoid"
  bottom: "sodlt"
  top: "sodlt_norm"
}
layer{
  type: "Python"
  name: "cut_roi"
  top: "roi"
  top: "prob_max"
  bottom: "sodlt_norm"
