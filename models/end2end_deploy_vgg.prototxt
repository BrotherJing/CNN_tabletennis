    pyramid_height: 2
  }
}
layer {
  name: "sodlt_fc6"
  type: "InnerProduct"
  bottom: "pool5"
  top: "sodlt_fc6"
  param {
    lr_mult: 5
    decay_mult: 1
  }
  param {
    lr_mult: 10
    decay_mult: 1
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "sodlt_fc6"
  top: "sodlt_fc6"
}
